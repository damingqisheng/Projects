<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="680.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AlbumsController">
   <children>
      <Label fx:id="userNameLB" layoutX="40.0" layoutY="31.0" text="Label" />
      <TableView fx:id="albumTV" layoutX="40.0" layoutY="77.0" prefHeight="218.0" prefWidth="624.0">
      </TableView>
      <TextField fx:id="albumNameTF" layoutX="39.0" layoutY="314.0" />
      <Button fx:id="createBtn" layoutX="248.0" layoutY="314.0" mnemonicParsing="false" onAction="#createAlbumHandler" text="Create" />
      <Button fx:id="renameBtn" layoutX="352.0" layoutY="314.0" mnemonicParsing="false" onAction="#renameAlbumHandler" text="Rename" />
      <Button fx:id="removeBtn" layoutX="466.0" layoutY="314.0" mnemonicParsing="false" onAction="#removeAlbumHandler" text="Remove" />
      <Label fx:id="errTipLB" layoutX="39.0" layoutY="353.0" textFill="RED" />
      <Button fx:id="logoutBtn" layoutX="583.0" layoutY="26.0" mnemonicParsing="false" onAction="#logoutHandler" text="Logout" />
      <Button fx:id="openBtn" layoutX="585.0" layoutY="314.0" mnemonicParsing="false" onAction="#openAlbumHandler" text="Open" />
   </children>
</AnchorPane>
